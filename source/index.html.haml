                        %polygon#Path-2{:fill => "#F5A623", :points => "15 21.8756299 25 25 21.4720159 15"}
              %figure{:style => "position: relative" }
                %img{ :src => story.image, :style => "position: relative" }
                .gradient
              .story-box--words
                %h5.orange= story.person
                %h2= story.title

      -# Business section
      %section.story-group.row.space-group-big.clearfix
        .row-padding.col-md-3.ta-right
          %h2.blue BUSINESS
        .col-md-9.story-group.story-group--list
          %div{ :class => "story-box story-box--main row", :href => "http://google.com", :title => "Google the Team" }
            %figure.col-lg-7
              %a{:href => "/"}
                %img{ :src => "images/big-image.jpg" }

            .story-box--words.clearfix.col-lg-5
              %a{:href => "/", :class => "space-group", :style => "display:block;"}
                %h2 Deep Divisions Emerge in Trump Administration as North Korea Threatens War
                %p While President Trump and Kim Jong-un spoke in war terms, officials told Americans and the world they could “sleep well”
                %date 4 hours ago


          - data.main.business.each do |story|
            %div{ :class => "story-box story-box--main row", :href => "http://google.com", :title => "Google the Team" }
              %figure.col-md-4
                %a{:href => "/"}
                  %img{:title => story.title, :src => story.image }
              .story-box--words.col-md-8
                %a{:href => "/", :class => "space-group", :style => "display:block;"}
