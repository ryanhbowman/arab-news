= partial "menu"

%article.big-container
  .row
    %article.col-xs-12.col-lg-9
      .row.row-padding
        -# MAIN STORY
        %section.story-group.col-xs-12.col-md-7.col-lg-12
          %div{ :class => "story-box story-box--main row", :href => "http://google.com", :title => "Google the Team" }
            %figure.col-lg-7.last-lg
              %a{:href => "/"}
                %img{ :src => "images/big-image.jpg" }
            .story-box--words.clearfix.col-lg-5
              %a{:href => "/", :class => "space-group", :style => "display:block;"}
                %h1
                  Deep Divisions Emerge in Trump Administration as North Korea Threatens War
                .story-box--info.clearfix
                  %h5.f-l.purple World
                  %date.f-r 34 minutes ago
                %p
                  While President Trump and Kim Jong-un spoke in war terms, officials told Americans and the world they could “sleep well”
              .story-box--package
                %h4
                  %a{:href => "/"}
                    Mattis: North Korea Attack Could Lead to War
                %h4
                  %a{:href => "/"}
                    Where to find counter-protests to next weekend’s ‘Free Speech Rally’
          -# %img{ :src => "images/leaderboard.jpg", :class=> "block-center" }

        -# SMALLER STORIES SECTION
        .row.space-group.col-xs-12.col-md-5.col-lg-12.space-md
          %section.story-group--lined.col-lg-4
            - data.main.small.each do |story|
              %a{ :class => "story-box story-box--tiny", :href => "http://google.com", :title => story.title }
                %figure
                  %img{:title => story.title, :src => story.image }
                %h3= story.title
                %date= story.date


          %section.story-group.story-group--lined.col-lg-8

            .row
              - data.main.medium.each do |story|
                .col-lg-6
                  %a{ :class => "story-box story-box--medium", :href => "http://google.com", :title => story.title }
                    %h2= story.title
                    .clearfix
                      %figure
                        %img{:title => story.title, :src => story.image }
                      %p= story.abstract
                      %date= story.date

      -# OPINION SECTION

      %section.story-group--swipe.space-group-big
        .row-padding
          %h2.orange OPINION

        .swipe-slick.row
          - data.main.opinion.each do |story|
            %a{ :class => "story-box story-box--swipe col-md-3", :href => "http://google.com", :title => story.title }
              .o-icon
                %svg{:height => "28px", :version => "1.1", :viewbox => "0 0 28 28", :width => "28px", :xmlns => "http://www.w3.org/2000/svg", "xmlns:xlink" => "http://www.w3.org/1999/xlink"}
